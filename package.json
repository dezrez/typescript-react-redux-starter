{
  "name": "typescript-react-redux",
  "version": "1.0.0",
  "description": "A boiler-plate setup for TypeScript/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "prebuild:uat": "npm run clean",
    "prebuild:live": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "build:uat": "cross-env NODE_ENV=production REDIRECT_URL=callcentre.dezrez.com REZI_ENV=uat webpack -p",
    "build:live": "cross-env NODE_ENV=production REDIRECT_URL=callcentre.dezrez.com REZI_ENV=live webpack -p",
    "clean": "rimraf dist/",
    "dev:server": "cross-env NODE_ENV=development PORT=8080 nodemon server/node-server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --https --progress --no-info",
    "dev": "concurrently -r \"npm run dev:client\"",
    "lint": "npm run lint-ts && npm run lint-js",
    "lint-css": "stylelint \"./src/**/*.scss\"",
    "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\" \"test/**/*.tsx\" \"test/**/*.ts\"",
    "lint-js": "eslint \"**/*.js\" --ignore-path .gitignore",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "test": "karma start --singleRun true",
    "test:watch": "karma start --singleRun false"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "starter",
    "seed",
    "dezrez",
    "rezi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dezrez/typescript-react-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/dezrez/typescript-react-redux-starter/issues"
  },
  "dependencies": {
    "express": "^4.13.4",
    "helmet": "^2.0.0",
    "http-proxy": "^1.13.2",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "@types/base16": "^1.0.1",
    "@types/chai": "^3.4.34",
    "@types/classnames": "0.0.32",
    "@types/core-js": "^0.9.35",
    "@types/enzyme": "^2.7.2",
    "@types/fetch-mock": "^5.8.0",
    "@types/lodash": "^4.14.43",
    "@types/moment": "^2.13.0",
    "@types/normalizr": "^2.0.18",
    "@types/react": "^0.14.51",
    "@types/react-addons-test-utils": "^0.14.15",
    "@types/react-dom": "^0.14.19",
    "@types/react-intl": "^2.2.2",
    "@types/react-redux": "^4.4.35",
    "@types/react-router": "^2.0.41",
    "@types/react-router-redux": "^4.0.35",
    "@types/redux-form": "^4.0.31",
    "@types/redux-mock-store": "0.0.7",
    "@types/redux-thunk": "^2.1.32",
    "@types/sinon": "^1.16.32",
    "@types/whatwg-fetch": "0.0.33",
    "autoprefixer": "^6.3.3",
    "awesome-typescript-loader": "^3.0.0-beta.13",
    "babel-polyfill": "^6.9.1",
    "body-parser": "^1.15.1",
    "bootstrap": "4.0.0-alpha.5",
    "chai": "^3.5.0",
    "classnames": "^2.2.3",
    "compression-webpack-plugin": "^0.3.2",
    "concurrently": "^3.0.0",
    "copy-webpack-plugin": "^3.0.1",
