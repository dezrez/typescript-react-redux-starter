    "fetch-mock": "^5.9.3",
    "html-webpack-plugin": "^2.9.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
