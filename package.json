{
  "name": "typescript-react-redux",
  "version": "1.0.0",
  "description": "A boiler-plate setup for TypeScript/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "prebuild:uat": "npm run clean",
    "prebuild:live": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "build:uat": "cross-env NODE_ENV=production REDIRECT_URL=callcentre.dezrez.com REZI_ENV=uat webpack -p",
    "build:live": "cross-env NODE_ENV=production REDIRECT_URL=callcentre.dezrez.com REZI_ENV=live webpack -p",
    "clean": "rimraf dist/",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon server/node-server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --https --progress --no-info",
    "dev": "concurrently -r \"npm run dev:client\"",
    "lint": "npm run lint-ts && npm run lint-js",
    "lint-css": "stylelint \"./src/**/*.scss\"",
    "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\" \"test/**/*.tsx\" \"test/**/*.ts\"",
    "lint-js": "eslint \"**/*.js\" --ignore-path .gitignore",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "pretest": "npm run lint",
    "test": "karma start --singleRun true",
    "test:watch": "karma start --singleRun false",
    "posttest": "istanbul check-coverage --statement 75 --branch 50 --function 50"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "starter",
    "seed",
	"dezrez",
	"rezi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dezrez/typescript-react-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/dezrez/typescript-react-redux-starter/issues"
  },
  "dependencies": {
    "express": "^4.13.4",
    "helmet": "^2.0.0",
    "http-proxy": "^1.13.2",
    "winston": "^2.1.1"
  },
